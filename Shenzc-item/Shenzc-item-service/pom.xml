        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>druid-spring-boot-starter</artifactId>
            <version>${druid.starter.version}</version>
        </dependency>
        <dependency>
            <groupId>com.shenzc.service</groupId>
            <artifactId>Shenzc-item-interface</artifactId>
            <version>${shenzc.shopping}</version>
        </dependency>
        <dependency>
            <groupId>com.shenzc.common</groupId>
            <artifactId>Shenzc-common</artifactId>
            <version>${shenzc.shopping}</version>
